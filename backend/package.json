{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npx tsx src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "seed": "ts-node src/scripts/seedUsers.ts",
    "db": "ts-node src/scripts/viewAllUsers.ts",
    "update-user": "ts-node src/scripts/updateUserProfile.ts",
    "db:chats": "ts-node src/scripts/viewChats.ts",
    "seed:chat-messages": "ts-node src/scripts/seedChatMessages.ts",
    "format": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "db:clear-chats": "ts-node src/scripts/clearChats.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "TypeScript backend project",
  "dependencies": {
    "@jest/globals": "^30.2.0",
    "@socket.io/component-emitter": "^3.1.2",
    "@types/socket.io": "^3.0.1",
    "dotenv": "^17.2.1",
    "express": "^4.18.0",
    "google-auth-library": "^10.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.24",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "mongo-express": "^1.1.0-rc-3",
    "nodemon": "^3.0.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
